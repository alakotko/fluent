{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node",
    "lib": [
      "ESNext"
    ],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noImplicitReturns": true
  },
  "files": []
}
